{
  "name": "maplibre-ui-translations",
  "version": "0.4.0",
  "description": "Community translations for the default MapLibre UI.",
  "author": "Sam Woodcock",
  "license": "MIT",
  "type": "module",
  "main": "dist/maplibre-ui-translations.umd.js",
  "module": "dist/maplibre-ui-translations.js",
  "types": "dist/maplibre-ui-translations.d.ts",
  "exports": {
    ".": {
      "import": "./dist/maplibre-ui-translations.js",
      "require": "./dist/maplibre-ui-translations.umd.js",
      "types": "./dist/maplibre-ui-translations.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:umd",
    "build:esm": "tsc",
    "build:umd": "rolldown -c",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "maplibre",
    "maplibre-gl",
    "translations",
    "i18n",
    "localization",
    "maps"
  ],
  "devDependencies": {
    "rolldown": "1.0.0-beta.42",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "maplibre-gl": ">=2.0.0"
  }
}