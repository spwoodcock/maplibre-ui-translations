{
  "name": "maplibre-ui-translations",
  "version": "0.1.1",
  "description": "Community translations for the default MapLibre UI.",
  "author": "Sam Woodcock",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:umd",
    "build:esm": "tsc",
    "build:umd": "rolldown -c",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "maplibre",
    "maplibre-gl",
    "translations",
    "i18n",
    "localization",
    "maps"
  ],
  "devDependencies": {
    "rolldown": "1.0.0-beta.42",
    "typescript": "^5.9.3"
  }
}
